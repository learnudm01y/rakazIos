{
  "name": "rakaz-ios-app",
  "version": "1.0.0",
  "description": "RAKAZ iOS Application - Capacitor Project",
  "author": "RAKAZ Store",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean": "rm -rf App/build App/DerivedData",
    "ios:build": "cd App && xcodebuild -project App.xcodeproj -scheme App -configuration Debug clean build",
    "ios:archive": "cd App && xcodebuild -project App.xcodeproj -scheme App -configuration Release archive -archivePath build/App.xcarchive"
  },
  "dependencies": {
    "@capacitor/app": "8.0.0",
    "@capacitor/browser": "8.0.0",
    "@capacitor/core": "8.0.0",
    "@capacitor/haptics": "8.0.0",
    "@capacitor/ios": "8.0.0",
    "@capacitor/keyboard": "8.0.0",
    "@capacitor/splash-screen": "8.0.0",
    "@capacitor/status-bar": "8.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.0"
  }
}

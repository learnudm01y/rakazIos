{
  "name": "rakaz-ios-app",
  "version": "1.0.0",
  "description": "RAKAZ iOS Application - Capacitor Project",
  "author": "RAKAZ Store",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean": "rm -rf App/build App/DerivedData",
    "ios:build": "cd App && xcodebuild -project App.xcodeproj -scheme App -configuration Debug clean build",
    "ios:archive": "cd App && xcodebuild -project App.xcodeproj -scheme App -configuration Release archive -archivePath build/App.xcarchive"
  },
  "dependencies": {
    "@capacitor/app": "^6.0.1",
    "@capacitor/browser": "^6.0.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/haptics": "^6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/keyboard": "^6.0.2",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "typescript": "^5.0.0"
  }
}
